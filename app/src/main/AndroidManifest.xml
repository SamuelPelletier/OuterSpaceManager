<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pellesam.outerspacemanager">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.pellesam.outerspacemanager.SignUpActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category
                    android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.pellesam.outerspacemanager.MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.pellesam.outerspacemanager.BuildingActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.pellesam.outerspacemanager.GeneralActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.pellesam.outerspacemanager.GalaxyActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.pellesam.outerspacemanager.SearchActivity"
            android:label="@string/app_name" >
        </activity>

    </application>
    <uses-permission android:name="android.permission.INTERNET" />
</manifest>
